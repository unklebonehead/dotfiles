#! /bin/sh
# custom dwm keybindings cheatsheet
notify-send -u normal -i \
"/usr/share/icons/Papirus-Dark/symbolic/devices/input-keyboard-symbolic.svg" \
"DWM keybindings:" \
"S + space	rofi\n\
S +  		term\n\
S + f		Files\n\
S + b		Firefox\n\
S + 1-9	go to ws\n\
S + Sh + 1-9	mv win to ws\n\
S + Sh + ,   mv win\n\
S + q		close win\n\
S + x		exit\n\
S + Sh + q	reload dwm\n\
"
